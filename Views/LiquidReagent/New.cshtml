@model LiquidReagentCreateViewModel
@{ 
    ViewData["Title"] = "Create Form";
}

<form>
    <div asp-validation-summary="ModelOnly"></div>
    <div class="form-group">
        <label for="@Model.LiquidReagent.Name">Name</label>
        <input class="form-control" asp-for="@Model.LiquidReagent.Name" placeholder="Enter Name">
        <span asp-validation-for="@Model.LiquidReagent.Name"></span>
    </div>
    <div class="form-group">
        <label for="@Model.LiquidReagent.AlertCode">Alert Code</label>
        <input class="form-control" asp-for="@Model.LiquidReagent.Name" placeholder="Enter Name">
        <span asp-validation-for="@Model.LiquidReagent.Name"></span>
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>